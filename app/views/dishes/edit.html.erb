<%#= render "form", dish: @dish, restaurant: @restaurant %>
<%= simple_form_for [@restaurant, @dish], url: "/dishes" do |f|%>
  <%= f.input :name, prompt: "Dish Name", input_html: {class:"form-control"} %>
  <%= f.input :price, prompt: "Price", input_html: {class:"form-control"} %>
  <%= f.input :description, prompt: "Dish Description", input_html: {class:"form-control"} %>
  <%= f.input :status, prompt: "status", input_html: {class:"form-control"} %>
  <%= f.submit "save", class:"btn btn-primary" %>
<% end %>
